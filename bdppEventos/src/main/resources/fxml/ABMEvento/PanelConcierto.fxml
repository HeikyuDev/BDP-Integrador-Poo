<VBox maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="173.0" prefWidth="485.0" xmlns="http://javafx.com/javafx/17.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.bdppeventos.controller.ABMEvento.PanelConciertoController">
    <children>
        <HBox alignment="CENTER" prefHeight="30.0" prefWidth="453.0" spacing="18.0">
            <children>
                <Label fx:id="lblConcierto" text="Concierto">
                    <font>
                        <Font size="15.0" />
                    </font>
                </Label>
            </children>
        </HBox>
        <HBox alignment="CENTER_LEFT" prefHeight="142.0" prefWidth="485.0" spacing="40.0">
         <children>
            <CheckBox fx:id="chkEsPago" mnemonicParsing="false" text="Es pago?" onAction="#configurarEsPago" />
            <TextField fx:id="txtEsPago" disable="true" editable="false" />
         </children>
        </HBox>
    </children>
</VBox>
